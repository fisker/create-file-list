{
  "name": "create-file-list",
  "version": "1.1.0",
  "description": "Create `FileList`",
  "keywords": [
    "FileList",
    "files"
  ],
  "repository": "fisker/create-file-list",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/create-file-list.min.js",
  "jsdelivr": "dist/create-file-list.min.js",
  "unpkg": "dist/create-file-list.min.js",
  "module": "dist/create-file-list.min.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "timeout": "1m",
    "verbose": true
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@commitlint/cli": "16.2.4",
    "@fisker/commitlint-config": "2.0.4",
    "@fisker/eslint-config": "10.1.4",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/prettier-config": "5.0.6",
    "@rollup/plugin-babel": "5.3.1",
    "ava": "4.2.0",
    "babel-plugin-transform-async-to-promises": "0.8.18",
    "create-test-server": "3.0.1",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "5.0.0",
    "eslint": "8.15.0",
    "husky": "7.0.4",
    "launch-puppeteer": "0.0.3",
    "lint-staged": "12.4.1",
    "markdownlint-cli": "0.31.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "rollup": "2.72.0",
    "rollup-plugin-terser": "7.0.2",
    "sort-package-json": "1.57.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
