{
  "name": "create-file-list",
  "description": "create FileList",
  "keywords": [
    "files",
    "FileList"
  ],
  "version": "1.0.0",
  "main": "lib/create-file-list.min.js",
  "module": "lib/create-file-list.min.mjs",
  "unpkg": "lib/create-file-list.min.js",
  "jsdelivr": "lib/create-file-list.min.js",
  "repository": "fisker/create-file-list",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf lib",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.4",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.6",
    "@fisker/prettier-config": "1.0.21",
    "ava": "2.4.0",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "create-test-server": "3.0.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.6.0",
    "esm": "3.2.25",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "puppeteer": "2.0.0",
    "rimraf": "3.0.0",
    "rollup": "1.26.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-terser": "5.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true,
    "require": "esm"
  },
  "nyc": {
    "all": true,
    "require": "esm",
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
